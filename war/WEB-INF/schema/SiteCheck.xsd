<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:site-check.appspot.com"
	xmlns:tns="urn:site-check.appspot.com" elementFormDefault="qualified">

	<element name="Account" type="tns:Account" />

	<complexType name="Account">
		<sequence>

			<element name="Host" id="HostCheck" type="tns:HostCheck"
				minOccurs="0" maxOccurs="unbounded" />

			<element name="User" type="tns:User" minOccurs="0" maxOccurs="1" />

		</sequence>
		<attribute name="accountName" type="tns:string80" use="required" />
	</complexType>

	<complexType name="User">
		<sequence>
			<element name="Email" type="tns:Email" minOccurs="0"
				maxOccurs="unbounded" />

			<element name="SMS" type="tns:SMS" minOccurs="0" maxOccurs="unbounded" />

		</sequence>
		<attribute name="name" type="tns:string80" use="required" />
		<attribute name="timeZone" type="tns:TimeZone" use="required" />
		<attribute name="role" use="required">
			<simpleType>
				<restriction base="tns:string20">
					<enumeration value="Admin" id="admin" />
					<enumeration value="Viewer" id="viewer" />
				</restriction>
			</simpleType>
		</attribute>
	</complexType>

	<simpleType name="EmailField">
		<restriction base="tns:string80">
			<!--  valid email chars .!#$%*/?|^{}`~&'+-=_ -->
			<pattern
				value="[_\.\-!#$%\*/\?|^{}`~&amp;'+=a-zA-Z0-9]+@([A-Za-z0-9]+)(([\.\-]?[a-zA-Z0-9]+)*)\.([A-Za-z]{2,9})">
				<annotation>
					<documentation><![CDATA[A valid e-mail address is needed (for example: joe@example.com)]]></documentation>
				</annotation>
			</pattern>

		</restriction>
	</simpleType>

	<complexType name="HostCheck">

		<attribute name="host" use="required">
			<annotation>
				<documentation><![CDATA[
                HTTP or HTTPS Address
                <ul>
                  <li><i>http://example.com</i></li> 
                  <li><i>https://example.com</i></li>
                </ul>  
                
                <p>You may include additional path information if you have 
                specific page that should be tested
                <ul> 
                  <li><i>http://example.com/myTestPage?site-check</i></li>
                </ul>
                <sub>+Other protocols will not work 
                    (ex: ftp, smtp, example.com:5432, etc..)</sub><p>]]></documentation>
			</annotation>
			<simpleType>
				<restriction base="tns:string160">
					<pattern
						value="http://([A-Za-z0-9]+)(([\.\-]?[a-zA-Z0-9]+)*)\.([A-Za-z]{2,9})([/\?].*)?"></pattern>
					<pattern
						value="https://([A-Za-z0-9]+)(([\.\-]?[a-zA-Z0-9]+)*)\.([A-Za-z]{2,9})([/\?].*)?"></pattern>
				</restriction>
			</simpleType>
		</attribute>
		<attribute name="alerts" type="boolean" default="true">
			<annotation>
				<documentation>
                    <![CDATA[If a host is constantly triggering 
                    alerts, this feature disables alerts for this host.]]>
				</documentation>
			</annotation>
		</attribute>
		<attribute name="frequency" default="10 Min">
			<annotation>
				<documentation>
                    <![CDATA[How often this host will be tested.]]>
				</documentation>
			</annotation>
			<simpleType>
				<restriction base="string">
					<enumeration value="1 Min" />
					<enumeration value="5 Min" />
					<enumeration value="10 Min" />
					<enumeration value="30 Min" />
					<enumeration value="1 Hr" />
				</restriction>
			</simpleType>

		</attribute>
		<attributeGroup ref="tns:HostSettings" />
		<attribute name="status" type="string">
			<annotation>
				<documentation>
                    <![CDATA[The last status]]>
				</documentation>
				<appinfo>
					<readonly />
				</appinfo>
			</annotation>
		</attribute>
		<attribute name="lastCheck" type="dateTime">
			<annotation>
				<documentation>
                    <![CDATA[The last time this host was checked.]]>
				</documentation>
				<appinfo>
					<readonly />
				</appinfo>
			</annotation>
		</attribute>
	</complexType>

	<attributeGroup name="HostSettings">
		<attribute name="fullRequest" default="Hourly" type="tns:HTTPFullRequest" />

		<attribute name="timeoutRecheck" default="Hourly">
			<annotation>
				<documentation>
                <![CDATA[Once a timeout is detected, this option
                    allows less frequent testing.  This allows the server
                    to make better use of resources.]]>
				</documentation>
			</annotation>
			<simpleType>
				<restriction base="string">
					<enumeration value="Hourly" />
					<enumeration value="Daily" />
					<enumeration value="Always" />
				</restriction>
			</simpleType>
		</attribute>
	</attributeGroup>

	<attributeGroup name="HostAlerts">
		<attribute name="offline" type="boolean" default="true">
			<annotation>
				<documentation><![CDATA[
            Notify when a host goes offline.
                ]]></documentation>
			</annotation>
		</attribute>
		<attribute name="online" type="boolean" default="false">
			<annotation>
				<documentation><![CDATA[
            Notify when a host comes back online.  This will double 
            the amount of notifications you will receive.
                ]]></documentation>
			</annotation>
		</attribute>
		<attribute name="timeout" type="boolean" default="false">
			<annotation>
				<documentation><![CDATA[
                Notify when this host does not respond within 10 
                seconds.  If your using shared-hosting, this option
                may trigger a lot of emails.
                ]]></documentation>
			</annotation>
		</attribute>
	</attributeGroup>

	<complexType name="SMS">
		<attribute name="provider" type="tns:SMSProvider" use="required" />
		<attribute name="phone" type="string" use="required" />
		<attribute name="enabled" type="boolean" default="true" />
		<attributeGroup ref="tns:HostAlerts" />
		<attribute name="verification" type="tns:verify" default="Pending" />
	</complexType>


	<complexType name="Email">
		<attribute name="name" type="tns:string40" use="optional" />
		<attribute name="email" type="tns:EmailField" use="required">
			<!--
				annotation> <appinfo> <noUpdate /> </appinfo> </annotation
			-->
		</attribute>
		<attribute name="enabled" type="boolean" default="true" />
		<attributeGroup ref="tns:HostAlerts" />
		<attribute name="verification" type="tns:verify" default="Pending" />
	</complexType>

	<simpleType name="verify">
		<annotation>
			<appinfo>
				<readonly />
			</appinfo>
			<documentation>
<![CDATA[NOTE: This address will be verified.]]>
			</documentation>
		</annotation>
		<restriction base="tns:string20">
			<enumeration value="Pending"></enumeration>
			<enumeration value="Verified"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="HTTPFullRequest">
		<annotation>
			<documentation>
                <![CDATA[A request is made when checking on your host. This
                option forces a full-page download. A full-page download
                allows the system to record the download speed.
                <p>You may check on a host more frequently than the
                value specified here. When this happens, a much more
                efficient HEAD request is used. The HEAD request tells
                the host it need only respond with the headers (which 
                includes the response status code).</p>]]>
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="Hourly">
				<annotation>
					<documentation><![CDATA[Only download the full page once an
                        hour.]]>
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="Daily">
				<annotation>
					<documentation><![CDATA[Only download the full page once a
                        day.]]>
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="Always">
				<annotation>
					<documentation><![CDATA[Always time and download the entire
                        page at my host.]]>
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="Never">
				<annotation>
					<documentation><![CDATA[I don't care about saving the page
                        download rate.]]>
					</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

